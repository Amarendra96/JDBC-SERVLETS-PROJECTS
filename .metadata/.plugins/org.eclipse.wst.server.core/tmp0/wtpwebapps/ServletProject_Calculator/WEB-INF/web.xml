<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>AddServlet</servlet-name>
		<servlet-class>com.jsp.servlet.AddServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SubstractServlet</servlet-name>
		<servlet-class>com.jsp.servlet.SubstractServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MultiplyServlet</servlet-name>
		<servlet-class>com.jsp.servlet.MultiplyServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DivideServlet</servlet-name>
		<servlet-class>com.jsp.servlet.DivideServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ModulusServlet</servlet-name>
		<servlet-class>com.jsp.servlet.ModulusServlet</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>AddServlet</servlet-name>
		<url-pattern>/AddServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SubstractServlet</servlet-name>
		<url-pattern>/SubstractServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MultiplyServlet</servlet-name>
		<url-pattern>/MultiplyServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DivideServlet</servlet-name>
		<url-pattern>/DivideServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ModulusServlet</servlet-name>
		<url-pattern>/ModulusServlet</url-pattern>
	</servlet-mapping>

</web-app>
